<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RelatorioCursoController">
   <children>
      <AnchorPane layoutX="-13.0" prefHeight="417.0" prefWidth="648.0" style="-fx-background-color: #61bdff;">
         <children>
            <Button fx:id="ConfirmBotao" layoutX="426.0" layoutY="90.0" mnemonicParsing="false" onAction="#ExibirTurmas" style="-fx-background-color: #ffffff;" text="OK" />
            <Label layoutX="162.0" layoutY="90.0" prefHeight="26.0" prefWidth="100.0" text="Nome do curso:" />
            <TextField fx:id="CursoNome" layoutX="262.0" layoutY="90.0" />
