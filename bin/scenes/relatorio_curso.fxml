               <font>
                  <Font name="Times New Roman" size="34.0" />
               </font>
            </Label>
            <TableView fx:id="TabelaTurma" layoutX="114.0" layoutY="168.0" prefHeight="224.0" prefWidth="446.0">
              <columns>
                <TableColumn fx:id="TurmaCodigo" prefWidth="75.0" text="Código" />
                <TableColumn fx:id="TurmaDia" prefWidth="75.0" text="Dia" />
